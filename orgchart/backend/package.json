{
  "name": "orgchart-backend",
  "version": "1.0.0",
  "description": "Backend API for organizational chart management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Backend build completed - no compilation needed'",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "sequelize",
    "jwt",
    "authentication",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.0-rc.4",
    "mysql2": "^3.14.2",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^7.0.5",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "superagent": "^10.2.2",
    "supertest": "^7.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
